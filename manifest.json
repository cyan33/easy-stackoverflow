{
  "name": "easy-stackoverflow",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A Simple Notifier and Search Shortcut for Stackoverflow",
  "homepage_url": "https://github.com/thomasyimgit/easy-stackoverflow",
  "author": "Chang Yan",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "128": "public/128.jpg"
  },
  "browser_action": {
    "default_icon": "public/icon.png",
    "default_popup": "public/index.html"
  }
}
